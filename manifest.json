{
    "manifest_version": 3,
    "name": "NovelAI Image Size Presets",
    "version": "1.0",
    "description": "Adds customizable preset image size buttons on novelai.net/image",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["storage"],
    "content_scripts": [
      {
        "matches": ["*://novelai.net/image*"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }
  